<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mia.cm.anand.tennisapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SecondActivity">
            <intent-filter>
                <action android:name="mia.cm.anand.tennisapp.SecondActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".ThirdActivity" />

        <intent-filter>
            <action android:name="mia.cm.anand.tennisapp.ThirdActivity" />

            <category android:name="android.intent.category.DEFAULT" />
        </intent-filter>

        <activity android:name=".about_us">
            <intent-filter>
                <action android:name="mia.cm.anand.tennisapp.about_us" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".TableAdapter" />
        <activity android:name=".DatabaseTable" />
        <activity android:name=".FourthActivity"></activity>
    </application>

</manifest>